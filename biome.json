{
    "linter": {
        "rules": {
            "suspicious": {
                "noDebugger": "off",
                "noConsoleLog": "info",
                "noExplicitAny": "off"
            },
            "style": {
                "noShoutyConstants": "warn",
                "useImportType": "off",
                "useNamingConvention": {
                    "options": {
                        "strictCase": false
                    }
                }
            }
        }
    },
    "javascript": {
        "parser": {
            "unsafeParameterDecoratorsEnabled": true
        }
    }
}